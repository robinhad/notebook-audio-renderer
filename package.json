{
  "name": "notebook-audio-playback",
  "displayName": "Notebook audio playback",
  "description": "Play WAV files in your notebook cell in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "notebook-audio-playback",
        "entrypoint": "./out/client/index.js",
        "displayName": "Notebook audio playback",
        "mimeTypes": [
          "text/html"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/vscode": "^1.68.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.16.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^2.1.3",
    "util": "^0.12.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
